  "html.filetypes": [
    "html",
    "htm",
    "handlebars",
    "blade",
    "ejs",
    "html.ejs"
  ],

  "htmlhint.documentSelector": [
    "html",
    "htm",
    "handlebars",
    "blade",
    "ejs",
    "html.ejs"
  ],

  "css.lint.universalSelector": "warning",
  "css.lint.duplicateProperties": "warning",
  "scss.lint.universalSelector": "warning",
  "scss.lint.duplicateProperties": "warning",

  "eslint.packageManager": "npm",
  "eslint.autoFixOnSave": false,

  "list.source.grep.command": "rg",

  "prettier.printWidth": 120,
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "none",
  "prettier.bracketSpacing": true,
  "prettier.jsxBracketSameLine": false,
  "prettier.parser": "none",
  "prettier.semi": true,
  "prettier.useTabs": false,
  "prettier.proseWrap": "preserve",
  "prettier.arrowParens": "avoid",

  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "disableWorkspaceFolders": true,
      "filetypes": ["go"]
    }
  },

  "markdownlint.config": {
    "no-trailing-spaces": {
      "br_spaces": 2
    },
    "no-multiple-blanks": {
      "maximum": 3
    },
    "no-bare-urls": false,
    "line-length": {
      "line_length": 300
    },
    "no-emphasis-as-heading": false,
    "no-inline-html": false
  },
  "snippets.priority": 200,
  "snippets.userSnippetsDirectory": "~/vimfiles/snippets",
  "snippets.extends": {
  },
  "snippets.shortcut": "SNIP",

  "highlight.colorNames.enable": false,
  "highlight.document.enable": false,
  "highlight.disableLanguages": ["javascript","javascriptreact"],

  "emmet.variables": {
    "lang": "ja"
  },
  "emmet.priority": 1,

  "explorer.icon.enableNerdfont": true,
  "explorer.file.showHiddenFiles": true
}
